msgstr "Lungimea ASN.1 nu se potrivește cu valoarea așteptată"

#. asn1_err.et:asn1:ASN1_BAD_LENGTH
#: ../src/dummy-strings.c:18
msgid "ASN.1 badly-formatted encoding"
msgstr "Codarea ASN.1 este formatată nepotrivit"

#. asn1_err.et:asn1:ASN1_BAD_FORMAT
#: ../src/dummy-strings.c:19
msgid "ASN.1 parse error"
msgstr "Eroare de analizare ASN.1 "

#. asn1_err.et:asn1:ASN1_PARSE_ERROR
#: ../src/dummy-strings.c:20
